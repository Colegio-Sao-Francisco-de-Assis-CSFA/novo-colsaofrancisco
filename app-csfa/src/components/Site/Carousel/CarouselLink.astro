---
import type { HTMLAttributes } from "astro/types";

interface Props extends HTMLAttributes<"a"> {
    id: string;
    href: string;
    classes?: string;
    iscontainer?: boolean;
}

const { id, href, classes, iscontainer, ...rest } = Astro.props;
---

<a
    href={href}
    id={id}
    class:list={[classes, { carousel__container: iscontainer }]}
>
</a>
